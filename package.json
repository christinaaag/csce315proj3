{
  "name": "project3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install server": "cd ../backend && npm install",
    "install client": "cd ../frontend && npm install",
    "install all": "concurrently --kill-others \"npm run install server\" \"npm run install client",
    "client": "cd ../frontend && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "cd ../backend && node server.js",
    "start all": "concurrently --kill-others \"npm run client\" \"npm run server\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "all": "0.0.0"
  }
}
